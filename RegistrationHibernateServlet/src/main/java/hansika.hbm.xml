<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//hibernate/hibernate mapping dtd2.0//en"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="mypojo" table="Registration">
<id name="uname" column="uname" type="string">
<generator class ="assigned"></generator></id>
<property name="course" type="string" column="course" />
<property name="fees" type="string" column="fees" />
<property name="phoneNo" type="string" column="phoneNo" />
<property name="email" type="string" column="email" />
</class>
</hibernate-mapping>